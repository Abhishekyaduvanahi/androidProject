<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            motion:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress"
            android:layout_marginBottom="76dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="333dp"
            android:id="@+id/textView3"
            motion:layout_constraintVertical_bias="0.629" />
        <Constraint
            android:layout_height="85dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="140dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.593"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="239dp"
            android:id="@+id/editTextTextEmailAddress" />
        <Constraint
            motion:layout_constraintVertical_bias="0.445"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.526"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress"
            android:id="@+id/button" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="333dp"
            android:id="@+id/textView3" />
    </ConstraintSet>
</MotionScene>